<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%date{yyyy-MM-dd'T'HH:mm:ss.SSSXXX}  %green(INFO) [%15.15threadName] : %h %u "%r" %s %b</pattern>
        </encoder>
    </appender>

    <appender-ref ref="STDOUT"/>
</configuration>
